using System;

public class CommonElements
{
    public static void Main(string[] args)
    {
        // First array
        int[] array1 = {1, 2, 3, 4, 5};

        // Second array
        int[] array2 = {3, 4, 5, 6, 7};

        Console.WriteLine("Common elements between the two arrays are:");
        for (int i = 0; i < array1.Length; i++)
        {
            for (int j = 0; j < array2.Length; j++)
            {
                if (array1[i] == array2[j])
                {
                    Console.WriteLine(array1[i]); // Print the common element
                }
            }
        }
    }
}


//output
Common elements between the two arrays are:
3
4
5
